    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "better-sqlite3": "^8.5.0",
    "isbot": "^4.1.0",
    "openai": "^4.73.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.0",
    "@types/react": "^18.2.20",
